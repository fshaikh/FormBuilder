{
  "name": "form-designer",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o -w -dev",
    "build-dev": "ng build",
    "build-noaot": "ng build --aot false --output-path ../../../build/FormDesigner --target -prod --env=prod",
    "build": "ng build --aot true --output-path ../../../build/FormDesigner --target -prod --env=prod",
    "build-sm": "ng build --aot true --output-path ../../../build/FormDesigner --target -prod --env=prod -sm",
    "test": "ng test",
    "lint": "ng lint --format prose > lintresult.txt",
    "e2e": "ng e2e",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts **/e2e/*.*' ./app/",
    "typedoc": "typedoc",
    "i18n": "./node_modules/.bin/ng-xi18n --i18nFormat=xlf --outFile=../../../i18n/FormDesigner"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.1.3",
    "@angular/cdk": "^2.0.0-beta.12",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.12",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "angular2-uuid": "^1.1.1",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.3",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typedoc": "^0.7.1",
    "typescript": "~2.2.0"
  }
}
